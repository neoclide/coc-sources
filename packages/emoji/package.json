{
  "name": "coc-emoji",
  "version": "1.2.2",
  "description": "emoji source for coc.nvim",
  "scripts": {},
  "engines": {
    "coc": ">=0.0.57"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoclide/coc-sources.git"
  },
  "keywords": [
    "coc.nvim",
    "emoji"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "coc.source.emoji.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.emoji.shortcut": {
          "type": "string",
          "default": "T"
        },
        "coc.source.emoji.triggerCharacters": {
          "type": "array",
          "default": [
            ":"
          ],
          "items": {
            "type": "string"
          }
        },
        "coc.source.emoji.priority": {
          "type": "integer",
          "default": 3
        },
        "coc.source.emoji.disableSyntaxes": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "coc.source.emoji.filetypes": {
          "type": [
            "array",
            "null"
          ],
          "default": [
            "markdown"
          ],
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "author": "chemzqm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoclide/coc-sources/issues"
  },
  "homepage": "https://github.com/neoclide/coc-sources#readme",
  "devDependencies": {
    "coc.nvim": ">=0.0.57"
  },
  "gitHead": "7ead63e794458e58e85d93556be8bf566ad1ceeb"
}
